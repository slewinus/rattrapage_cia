[req]
default_bits = 2048
prompt = no
default_md = sha256
req_extensions = v3_req
distinguished_name = dn

[dn]
CN = localhost

[v3_req]
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
DNS.2 = app.localhost
DNS.3 = api.localhost
DNS.4 = grafana.localhost
DNS.5 = portainer.localhost
DNS.6 = gitea.localhost
DNS.7 = traefik.localhost
IP.1 = 127.0.0.1
IP.2 = ::1
