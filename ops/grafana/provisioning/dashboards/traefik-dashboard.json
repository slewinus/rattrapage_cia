{
  "annotations": {"list": []},
  "editable": true,
  "graphTooltip": 0,
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests by Entrypoint (rps)",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "sum(rate(traefik_entrypoint_requests_total[5m])) by (entrypoint)", "legendFormat": "{{entrypoint}}"}
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "type": "timeseries",
      "title": "5xx by Router (rps)",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "sum(rate(traefik_router_requests_total{code=~\"5..\"}[5m])) by (router)", "legendFormat": "{{router}}"}
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "type": "table",
      "title": "Top Services by RPS",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "topk(10, sum(rate(traefik_service_requests_total[5m])) by (service))"}
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "type": "logs",
      "title": "Traefik Logs",
      "datasource": {"type": "loki", "uid": "Loki"},
      "targets": [
        {"expr": "{container=\"cia-ops-traefik-1\"}"}
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    }
  ],
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["traefik"],
  "templating": {"list": []},
  "time": {"from": "now-6h", "to": "now"},
